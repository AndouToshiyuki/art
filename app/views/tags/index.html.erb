<%= search_form_for @search, url:tags_path do |f| %>
  <%= f.label :tag_name,"タグ名" %>
  <%= f.search_field :tag_name_cont, class: 'search',placeholder: '何かお探しですか？' %>
  <%= f.submit "検索" %>
<% end %>
<div>
  <% @results.each do |tag|%>
  / <%=link_to tag.tag_name, tag_path(tag) %>
  <% end %>
</div>
